#include<stdio.h>


int finding(int pos_x,int pos_y,int pos_color,int chess[9][9]);

int main(){
    int chess[9][9];
    int pos_x=0,pos_y=0,pos_color=0;
    for(int i=0;i<8;i++){
        for(int j=0;j<8;j++)
            scanf("%d",&chess[i][j]);
    }

    scanf("%d%d%d",&pos_x,&pos_y,pos_color);

//Q1
    if(chess[pos_x][pos_y]==0){
        printf("its empty");
    }
    else if(chess[pos_x][pos_y]==1){
        printf("its black");
    }
    else{
        printf("its white");
    }

//Q2
    int find=1;
    while(find){
        if(chess[pos_x][pos_y]==0){

        }
        else{

        }
    }

}



int finding(int pos_x,int pos_y,int pos_color,int chess[9][9] ){
    int x=pos_x,y=pos_y;
    int revers_color=0,blank=2;

    if(pos_color==1){
        int revers_color=2;
    }
    else if(pos_color==2){
        int revers_color=1;
    }
    int last_color =pos_color;
    if(pos_color!=0){
        while(y<8){//right
            int x=pos_x,y=pos_y;
            y++;
            if(chess[x][y]==0){
                continue;
                last_color=0;
                blank=1;

            }
            else if(chess[x][y]==pos_color){
                if(last_color==revers_color && chess[x][y]==0 && blank==0){//((fix==1 && blank==1)||blank==0)如果經過空白，是否有補上同色旗，如果未經空白上一格為不同色可直接下
                    return 1;
                }
                last_color=pos_color;
                blank=0;

            }else if(chess[x][y]==revers_color){
                last_color=revers_color;
            }
        }

    }

}
