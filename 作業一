#include <stdio.h>

int main() {
    int ans=0;
    for(int k=1;k<=3;k++){
        for(int i=1;i<=9;i++){
            for(int j=k; j<=9;j=j+3){
                ans=i*j;
                printf("%dx%d=%d\t",j,i,ans);
            }
            printf("\n");
        }
        printf("\n");
    }
    

    return 0;
}
